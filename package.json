{
  "name": "spacekit",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --minify --sourcemap --target=es2015 --global-name=Spacekit --tsconfig=tsconfig.json --outfile=build/spacekit.js",
    "build:watch": "esbuild src/index.ts --bundle --minify --sourcemap --target=es2015 --global-name=Spacekit --tsconfig=tsconfig.json --watch --outfile=build/spacekit.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "docs": "typedoc",
    "format": "prettier --write --single-quote --trailing-comma all '{src,examples,test}/**/*.{js,ts}'",
    "test": "jest"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.6.3",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.15.0",
    "@types/node": "^16.7.10",
    "@types/three": "^0.131.0",
    "@typescript-eslint/parser": "^4.14.0",
    "babel-jest": "^25.5.1",
    "esbuild": "^0.12.25",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "jest": "^25.4.0",
    "prettier": "^2.3.2",
    "typedoc": "^0.21.9",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "julian": "^0.2.0",
    "postprocessing": "^6.22.5",
    "three": "^0.132.0"
  }
}
